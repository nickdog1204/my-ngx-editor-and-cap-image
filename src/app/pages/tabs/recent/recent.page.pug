doctype html
ion-header
  ion-toolbar(color='primary')
    ion-title 最近相片
ion-content
  ion-grid
    ion-row
      ion-col(size='4', *ngFor="let photo of photoList; let idx = index")
        ion-img([src]='photo.webviewPath', (click)='openPreview(photo, idx)')
  ion-fab(vertical="bottom", horizontal="center", slot='fixed')
    ion-fab-button((click)='captureImageAsync()' )
      ion-icon(name='camera')
